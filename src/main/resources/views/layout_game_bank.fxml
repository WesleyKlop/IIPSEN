      xmlns="http://javafx.com/javafx"
      fx:controller="client.ui.LayoutBankController"
      prefHeight="400.0" prefWidth="600.0"
      fx:id="rootPane">

    <ImageView fx:id="stack">
        <Image url="/cards/FALSE/BACK.png"/>
    </ImageView>
    <VBox fx:id="openCards">
        <ImageView fx:id="card0"></ImageView>
        <ImageView fx:id="card1"></ImageView>
        <ImageView fx:id="card2"></ImageView>
        <ImageView fx:id="card3"></ImageView>
        <ImageView fx:id="card4"></ImageView>
    </VBox>

</VBox>
