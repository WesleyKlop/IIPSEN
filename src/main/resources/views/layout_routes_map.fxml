<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<Pane xmlns:fx="http://javafx.com/fxml"
      xmlns="http://javafx.com/javafx"
      fx:controller="client.ui.GameRoutesMapController"
      prefHeight="1080" prefWidth="1920.0"
      stylesheets="">

    <ImageView>
        <Image url="/images/Background.png"/>
    </ImageView>
    <Pane fx:id="mainPane" prefWidth="1920" prefHeight="1080"/>
    <Pane fx:id="informationPane" prefWidth="1920" prefHeight="1080"/>
    <Label fx:id="score" onMouseEntered="#ScoreEntered" onMouseExited="#ScoreExited" layoutX="870" layoutY="10"
           styleClass="routePointsButton" text="Points per route"/>
</Pane>
