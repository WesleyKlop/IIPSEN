<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="800.0" prefWidth="800.0" spacing="5" xmlns="http://javafx.com/javafx/8.0.121"
      fx:controller="client.ui.PreferencesController">
    <Label text="Fill in your name"/>
    <TextField fx:id="nameField"/>
    <fx:include source="layout_color_preference.fxml"/>


    <Button fx:id="submitPreferencesButton" onAction="#submitName" text="Submit"/>

</VBox>
